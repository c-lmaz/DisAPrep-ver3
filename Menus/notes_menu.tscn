theme_override_font_sizes/bold_italics_font_size = 20
bbcode_enabled = true
text = "[b]Sign of Sprains[/b]
1. [b]Pain[/b] around the injured joint.
2. [b]Swelling[/b] and tenderness.
3. [b]Bruising[/b] or discolouration.
4. [b]Difficulty moving[/b] the joint.
5.  [b]Instability[/b] or feeling 'looseness' in the joint."
fit_content = true

