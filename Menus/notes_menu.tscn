theme_override_font_sizes/italics_font_size = 18
theme_override_font_sizes/bold_italics_font_size = 20
bbcode_enabled = true
text = "[b]Causes of Sprains[/b]
1. Sudden [b]twisting or turning[/b].
2. [b]Falling[/b] and landing in an awkward position.
3. [b]Direct impact[/b] on a joint.
4. repeated stress on a joint due to [b]overuse[/b] or repetitive movements."
fit_content = true

[node name="RichTextLabel3" type="RichTextLabel" parent="ScrollContainer/VBoxContainer/FirstAidN/SprainN"]
